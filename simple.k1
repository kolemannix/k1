fn main(): i32 { 
   println("Size of StringBuilder: {types/sizeOf[core/StringBuilder]()}");

   let* sb = core/StringBuilder/new();
   for IntRange/upTo(128) do {
     if it % 2 == 0 sb.writeChar('a')
     else sb.writeChar('b');
   };
   let s = sb.buildTmp();
   assertEquals(s.len(), 128);
   assertEquals(s.get(0), 'a');
   assertEquals(s.get(127), 'b');
   println("{1} {2} {3}");
  0
}
