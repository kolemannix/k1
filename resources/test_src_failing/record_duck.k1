type Duck = {
  quack: int,
  waddle: int
}

// For this to actually work, we'd have to codegen a version of this function
// foreach time it is called, or do vtables.
// But at least it typechecks!
fn quack(d: { quack: int}): int {
  return d.quack;
}

fn main(): int {
  let duck: Duck = {
    quack: 555
  };
  printIt(quack(duck));
  return 0;
}
