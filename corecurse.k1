deftype Red[T] = { t: T, b: Black[T]* }
deftype Black[T] = {  t: T, r: Red[T]* }

fn returnRed[T](t: T): Red[T] {
  let rNull: Red[T]* = core/Pointer/NULL as Red[T]*;
  let b: Black[T]* = core/mem/new({ t: t, r: rNull });
  let r: Red[T] = { t: t, b: b };
  r
}

fn test(): i32 {
  returnRed[int](12);
  0
}

