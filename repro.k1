let static INT_BUFFER: Buffer[int] = [1,2,3,4,5].toBuffer();

fn test(): unit {
  // Buffer
  let buf: Buffer[int] = #static {
    let xs = [1,2,3,4,5];
    xs.toBuffer()
  };
  assert(buf == [1,2,3,4,5].toBuffer());
  let bufFromStatic = #static {
    let* l = List/wrapBuffer(INT_BUFFER);
    l.push(9);
    assertEquals(l.len, 6);
    assertEquals(l.last(), some(9));
  };
}

fn main(): i32 {
  test();
  0
}
