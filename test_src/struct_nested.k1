deftype Block = {
  position: { x: int, y: int, width: int, height: int },
  text: int
}

fn block_area(b: Block): int {
  b.position.width * b.position.height
}

fn main(): int {
  let b: Block = {
    position: { x: 1, y: 1, width: 50, height: 50 },
    text: 42
  };
  let area: int = block_area(b);
  assert(area == 2500);
  let area2: int = block_area({ position: { x: 1, y: 1, width: 50, height: 50 }, text: 42 });
  assert(area2 == 2500);
  0
}
