deftype E = enum A(int) , B
namespace E {
  fn value_if_a(self: E): int {
    switch self {
      , .A(i) -> i
      , .B -> crash("This is a B!")
    }
  }
}
fn main(): int {
  val x: E = .A(42);
  val y: E.A = .A(42);
  assert(x.value_if_a() == y.value_if_a());
  assert((.B: E).value_if_a() == 0);
  0
}
