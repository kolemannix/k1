type Foo = { x: int, y: bool }

namespace Foo {
  fn getValue(self: Foo): int {
    self.x
  }
  fn setValue(self: Foo*, x: int): unit {
    self.x = x;
  }
  fn new(x: int): Foo {
    { x: x, y: false }
  }
}

fn main(): int {
  val x: Foo* = Foo::new(42).&;
  assert(x.x == 42);
  assert(x.getValue() == 42);
  x.setValue(41);
  assert(x.getValue() == 41);
  assert(not x.y);
  0
}
//exitcode: 0
