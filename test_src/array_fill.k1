fn main(): int {
  let xs = Array::new[int]().&;
  assert(xs.cap == 0);
  assert(xs.len == 0);

  mut i = 0: u64;
  while i < 5 {
    assert(xs.len == i);
    xs.push(i as i64);
    i = i + 1;
  };
  assert(xs.len == 5);
  assert(xs.get(0) == 0);
  assert(xs.get(1) == 1);
  assert(xs.get(2) == 2);
  assert(xs.get(3) == 3);
  assert(xs.get(4) == 4);
  assert(xs.first()! == 0);
  assert(xs.last()! == 4);
  assert(xs.* == [0,1,2,3,4]);

  let xs2 = xs.cloned();
  assert(xs2 == xs.*);

  let empty = []: Array[byte];
  assert(empty.cloned() == empty);

  0
}
