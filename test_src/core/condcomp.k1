ns condcomp;

let const MY_VALUE: bool = true;
let const MY_VALUE_NEG: bool = not MY_VALUE;

fn test(): unit {
  println("K1_TEST is \{K1_TEST}");
  println("K1_OS is \{K1_OS}");

  assert(K1_OS == "macos");
  assert(K1_TEST == false);

  #if MY_VALUE_NEG {
    crash("does not happen")
  } else {
    println("hello from MY_VALUE_NEG=\{MY_VALUE_NEG}")
  };

  let comptimeCheck = #if false "foo" else "bar";
  assert(comptimeCheck == "bar");

  ()
}
