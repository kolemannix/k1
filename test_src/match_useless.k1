deftype Foo = { x: int, y: bool? }
fn main(): int {
  val foo: Foo = { x: 1, y: .None };
  val y = switch foo {
    { x, y: .Some(true) } -> x,
    { x, y: .Some(false) } -> x,
    { x, y: .Some(false) } -> x,
    { x, y: .None } -> x + 2
  };
  assert(y == 3);
  0
}
//errmsg: Useless pattern: { x: x, y: Some(false) }
