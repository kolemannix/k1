namespace match_useless {
deftype Foo = { x: int, y: bool? }
fn main(): int {
  let foo: Foo = { x: 1, y: .None };
  let y = switch foo {
    { x, y: .Some(true) } -> x,
    { x, y: .Some(false) } -> x,
    { x, y: .Some(false) } -> x,
    { x, y: .None } -> x + 2
  };
  assert(y == 3);
  0
}

} // end namespace
//errmsg: Useless pattern: { x: x, y: Some(false) }
