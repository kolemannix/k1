ns function_pointer;

fn staticAddOne(x: int): int { x + 1 }
fn staticAddTwo(x: int): int { x + 2 }

fn test(): unit {
  assert(staticAddOne.toRef() as ptr != staticAddTwo.toRef() as ptr);
  let someAddFn = staticAddOne.toRef();
  let x = someAddFn(10);
  assertEquals(x, 11);
  someAddFn := staticAddTwo.toRef();
  let y = someAddFn(20);
  assertEquals(y, 22);

  ()
}
