ns static_literals;
fn test(): unit {
  assertEquals(testCompile(1: 5), .Some("Expression did not conform to hint: Static lift resulted in wrong value: Different static values of same type: 5iword vs 1iword"));
  let c: 'c' = 'c';
  let pi: 3.14 = 3.14;
  let hello: "hello" = "hello";

  let hello: "hello!" = #static {
    let* bytes = [104u8];
    bytes.push(101);
    bytes.push(108);
    bytes.push(108);
    bytes.push(111);
    bytes.push(33);
    string/fromBytes(bytes.asView())
  };

  ()
}
