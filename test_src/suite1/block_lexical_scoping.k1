ns lexical_scope;

fn test(): unit {
  let result = testCompile({
    { let foo = 100 };
    assert(foo == 100);
  });
  println("{result}");
  assert(
    result is .Some(msg) 
    and msg.startsWith("Invalid call to assert\nInvalid type for parameter 'value': Variable 'foo' is not defined")
  )
}
