ns ability_constraint_fail;

ability Num {
  fn num(self: Self): i32
}

fn add[T](a: T, b: T): i32 where T: Num, T: Show {
  println(a.show());
  println("+");
  println(b.show());
  a.num() + b.num()
}

fn test(): unit {
  let result = testCompile(add("asdf", "asdf"));
  assertEquals(
    result,
    .Some("Provided type for T is string which does not implement required ability Num. Therefore, cannot call function 'add' with given types: T := string")
  )
}
