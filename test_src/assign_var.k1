fn foo(): int {
  let* result = 0;
  if true {
    referenceSet(result, 11);
  } else {
    referenceSet(result, 12);
  };
  result
}

fn main(): int {

  assert(foo() == 11);

  let* count = 0;
  referenceSet(count, 1);
  assert(count.* == 1);
  referenceSet(count, 2);
  assert(count.* == 2);
  0
}
