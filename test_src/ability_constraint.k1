ability Num {
  fn num(self: Self): i32
}

impl Show for i64 {
  fn show(self: i64): string { "todo" }
}

fn add[T](a: T, b: T): i32 where T: Num, T: Show {
  a.show();
  b.show();
  a.num() + b.num()
}

impl Num for int {
  fn num(self: int): i32 {
    self as i32
  }
}

fn main(): int {
  add(1, 2);
  0
}
