ns ability_constraint {

ability Num {
  fn num(self: Self): i32
}

fn add[T](a: T, b: T): i32 where T: Num, T: Show {
  println(a.show());
  println("+");
  println(b.show());
  a.num() + b.num()
}

impl Num for int {
  fn num(self: Self): i32 {
    self as i32
  }
}

}

fn main(): int {
  ability_constraint/add(1, 2);
  0
}
