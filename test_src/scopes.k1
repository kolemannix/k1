deftype Foo = { x: int }
deftype alias Alias = Foo

namespace Outer {
  deftype Foo = { x: int }
  namespace Foo {
    fn value(self: Foo): int { self.x + 3 }
  }
}

namespace Foo {
  fn value(self: Foo): int { self.x }
}


let x: int = 3;
fn scopeCollision(x: int): int {
  let x: int = 42;
  assert(x == 42);
  0
}

// Tests that the method gets assigned to the right Foo::value()
fn namespaceAlias(): unit {
  let x: Alias = {x:1};
  assert(x.value() == 1);
}

fn main(): int {
  scopeCollision(12);
  namespaceAlias();
  0
}
